/**
 * https://leetcode.com/problems/array-partition/
 *
 * @param {number[]} nums
 * @return {number}
 */
var arrayPairSum = function(nums) {
    
    // sort array
     nums.sort(function(a,b) {return a - b}) ;
    
    let sum = 0  ;
    let len = nums.length -1 ;
    
    // loop through 
   for( let i = 0; i < len  ;  i +=2) {
        
       sum += nums[i] ;    
                
    }  
    
    return sum ;
};
